{
  "name": "xmflsct-website",
  "private": true,
  "scripts": {
    "dev": "yarn dev:11ty & yarn dev:css",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i css/tailwind.css -o _site/css/tailwind.css --watch",
    "build": "yarn build:11ty && yarn build:css",
