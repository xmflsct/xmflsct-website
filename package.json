  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@tailwindcss/cli": "^4.1.18",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-link-attributes": "^4.0.1",
    "tailwindcss": "^4.1.18"
  },
  "packageManager": "yarn@4.5.1"
}
